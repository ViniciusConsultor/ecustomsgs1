<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <appSettings>
    <add key="serverURL" value="127.0.0.1" />
    <add key="serverPort" value="8042" />
    <add key="clientPort" value="0" />
    <add key="Disabled Activities" value="Person Description Basic" />
    <add key="HelpPath" value="file://c:/temp/Handbuch" />
    <add key="XmlPath" value="file://c:/work/.net/PIS/server/bin/debug/xml/ApplicationDefinition.xml" />
    <add key="ApplicationsAtServer" value="true" />
    <add key="DisplayUserRights" value="true" />
    <add key="StepByStepTrace" value="false" />
    <add key="ExternalTime" value="Timetrack Zeiterfassung"/>
    <add key="TopMost" value="false" />

    <!--setting regarding the cahche -->
    <!-- all these values are overwritten by the values specefied in appLauncher config, so , if the launcher is setted to use a cache, 
		the promova.exe will also use a cache for it's remote assemblies retrival,at the same location as the remoteApp launcher-->
    <add key="useClientChache" value="true" />

    <!--  if left empty, default value is to by used (%APPDATA%\[appName]\[cacheFolderName]), otherwise use a full path to the desired locaiton ( like c:\Window\TEMP) -->
    <add key="clientCacheLocation" value=""/>
    <add key="cacheFolderName" value="Cache"/>

    <add key="DisplayApplicationInfo" value="true" />
  </appSettings>
</configuration>