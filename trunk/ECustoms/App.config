<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <configSections>
    <section name="log4net" type="log4net.Config.Log4NetConfigurationSectionHandler, log4net" />
  </configSections>

  <appSettings>            
    <add key="SecurityKey" value="EcustomsSecurityKey"/>
    <!--Tự động làm mới dữ liệu theo giay -->
    <add key="TimeDelay" value="30" />
    <add key="IsDebugMode" value="1"/>
    
  </appSettings>
  <connectionStrings>
    <add name="dbEcustomEntities" connectionString=""
      providerName="System.Data.EntityClient" />
  </connectionStrings>

  <log4net debug="false">
    <appender name="LogFileAppender" type="log4net.Appender.FileAppender,log4net" >
      <param name="File" value="Logs\\EcustomsLog.txt" />
      <param name="AppendToFile" value="true" />
      <layout type="log4net.Layout.PatternLayout,log4net">
        <param name="ConversionPattern" value="%d [%t] %-5p %c [%x] &lt;%X{auth}&gt; - %m%n" />
      </layout>
    </appender>
    <root>
      <level value="Error"></level>
      <priority value="1" />
      <appender-ref ref="LogFileAppender" />
    </root>
  </log4net>

</configuration>